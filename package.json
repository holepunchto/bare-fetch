    "bare-http1": "^4.0.2",
    "bare-https": "^2.0.0",
    "bare-stream": "^2.6.0"
  },
  "devDependencies": {
    "bare-buffer": "^3.0.2",
    "brittle": "^3.3.0",
    "listen-async": "^1.0.0",
    "prettier": "^3.3.3",
    "prettier-config-standard": "^7.0.0"
  },
