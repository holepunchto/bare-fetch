{
  "name": "bare-fetch",
  "version": "2.2.2",
  "description": "WHATWG Fetch implementation for Bare",
  "exports": {
    "./package": "./package.json",
    ".": {
      "types": "./index.d.ts",
      "default": "./index.js"
    },
    "./global": {
      "types": "./lib/global.d.ts",
      "default": "./lib/global.js"
    },
    "./errors": {
      "types": "./lib/errors.d.ts",
      "default": "./lib/errors.js"
    },
    "./body": {
      "types": "./lib/body.d.ts",
      "default": "./lib/body.js"
    },
    "./request": {
      "types": "./lib/request.d.ts",
      "default": "./lib/request.js"
    },
    "./response": {
      "types": "./lib/response.d.ts",
      "default": "./lib/response.js"
    },
    "./headers": {
      "types": "./lib/headers.d.ts",
      "default": "./lib/headers.js"
