{
  "name": "bare-fetch",
  "version": "2.0.0",
  "description": "Minimal WHATWG Fetch implementation for Bare",
  "exports": {
    ".": "./index.js",
    "./package": "./package.json",
    "./global": "./global.js",
    "./errors": "./lib/errors.js",
    "./response": "./lib/response.js",
    "./headers": "./lib/headers.js"
  },
  "imports": {
    "http": {
      "bare": "bare-http1",
      "default": "http"
    },
    "https": {
      "bare": "bare-https",
      "default": "https"
    }
  },
  "files": [
    "index.js",
    "global.js",
    "lib"
  ],
  "scripts": {
    "test": "prettier . --check && bare test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/holepunchto/bare-fetch.git"
  },
  "author": "Holepunch",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/holepunchto/bare-fetch/issues"
  },
  "homepage": "https://github.com/holepunchto/bare-fetch#readme",
  "dependencies": {
    "bare-http1": "^4.0.0",
    "bare-https": "^2.0.0",
    "bare-stream": "^2.4.1"
  },
  "devDependencies": {
    "brittle": "^3.3.0",
    "listen-async": "^1.0.0",
    "prettier": "^3.3.3",
    "prettier-config-standard": "^7.0.0"
  }
}
